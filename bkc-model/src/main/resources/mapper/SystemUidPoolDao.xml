<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.bitflash.dao.SystemUidPoolDao">

    <select id="selectUid"
            statementType="CALLABLE" useCache="false"
            resultType="java.lang.String">
        <![CDATA[
		  call register(#{mobile,jdbcType=varchar,mode=IN},#{pwd,jdbcType=varchar,mode=IN},#{uid_out,jdbcType=varchar,mode=OUT})
		 ]]>
    </select>


</mapper>